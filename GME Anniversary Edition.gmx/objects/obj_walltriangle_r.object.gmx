            <kind>1</kind>
            <string>///Make the player run on walls

//If the player is not riding anything and does not have the cat powerup
if (global.mount == 0) 
&amp;&amp; (global.powerup != cs_cat) {

    //If the player meets the following conditions
    if (other.state == 1)
    &amp;&amp; (other.crouch == 0)
    &amp;&amp; (other.holding == 0)
    &amp;&amp; (other.swimming == 0) 
    &amp;&amp; (other.disablecontrol == 0) {
    
        //If the player is moving right and there's a wall at the right, run upwards
        if (other.xspeed &gt; 2.6)
        &amp;&amp; (position_meeting(x+24,y-8,obj_solid))        
            instance_create(x,y,obj_wallrunner);
            
        //Otherwise, if the player is moving left and there's a downwards wall there and no floor, run downwards.
        else if (other.xspeed &lt; -2.6)
        &amp;&amp; (position_meeting(x+8,y+24,obj_solid))
        &amp;&amp; (!position_meeting(x-8,y+24,obj_semisolid)) {
        
            with (instance_create(x-16,y+16,obj_wallrunner)) {
            
                direct = -1;
                direction = 270;
            }
        }
    }
}
</string>
          </argument>
