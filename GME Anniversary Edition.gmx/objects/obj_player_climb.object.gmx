            <string>///Player climb logic

//Update sprite
scr_mario_climb();

//If the player is about to climb up
if (ready == false) {

    //If the beanstalk does not exist
    if (!instance_exists(myvine)) {
    
        //Make the player climb up
        ready = true;
    
        //Move up
        vspeed = -1;
        
