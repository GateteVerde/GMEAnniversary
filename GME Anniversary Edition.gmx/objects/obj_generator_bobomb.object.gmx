        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Generate Bob-Ombs if inside the coordinates

//If the player exists
if (instance_exists(obj_playerparent)) {

    //If the player is inside the given 
    if (obj_playerparent.x &gt;= xmin)
    &amp;&amp; (obj_playerparent.x &lt;= xmax) {
    
        //If the level control is in silver coin mode, wait
        if (obj_levelcontrol.gswitchon) {
        
            //Hold event
            alarm[0] = 1;
            exit;
        }
        
        //Otherwise, spawn a bobomb
        else {
        
            //Generate a bobomb
            instance_create(view_xview[0]+random_range(32,global.gw-32),view_yview[0]-32,obj_bobomb_parachute);
            
            //Repeat
            alarm[0] = 180;
        }
    }
    
    //Otherwise, hold it
    else {
    
        //Hold event
        alarm[0] = 1;
        exit;
    }
}

//Otherwise, hold
else {

    //Hold event
    alarm[0] = 1;
    exit;
}
</string>
