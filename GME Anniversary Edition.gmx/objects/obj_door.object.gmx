    //If the player is overlapping the door
    var mario = collision_rectangle(bbox_left,bbox_top,bbox_right,bbox_bottom,obj_mario,0,0);
    if (mario)
    &amp;&amp; (mario.state &lt; 2)
    &amp;&amp; (keyboard_check(global.upkey)) {
    
